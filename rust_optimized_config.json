{
  "ollama_base_url": "http://localhost:11434",
  "ollama_model": "dengcao/Qwen3-Embedding-0.6B:F16",
  "qdrant_url": "http://localhost:6333",
  "embedding_length": 1024,
  "chunking_strategy": "treesitter",
  "token_chunk_size": 1024,
  "token_chunk_overlap": 120,
  "use_tree_sitter": true,
  "tree_sitter_max_file_size_bytes": 262144,
  "tree_sitter_max_blocks_per_file": 50,
  "tree_sitter_max_functions_per_file": 30,
  "tree_sitter_max_classes_per_file": 20,
  "tree_sitter_max_impl_blocks_per_file": 25,
  "tree_sitter_skip_test_files": true,
  "tree_sitter_skip_examples": true,
  "use_mmap_file_reading": true,
  "mmap_min_file_size_bytes": 65536,
  "auto_extensions": true,
  "exclude_files_path": null,
  "timeout_log_path": "rust_timeout_files.txt",
  "max_file_size_bytes": 524288,
  "batch_segment_threshold": 30,
  "search_min_score": 0.1,
  "search_max_results": 10,
  "embed_timeout_seconds": 120,
  "rust_specific_optimizations": {
    "skip_large_rust_files": true,
    "max_rust_file_size_kb": 300,
    "skip_generated_rust_files": true,
    "rust_target_directories": ["target/", "build/", "dist/"]
  },
  "tree_sitter_debug_logging": true,
  "search_file_type_weights": {
    ".vue": 1.85,
    ".ts": 1.45,
    ".tsx": 1.25,
    ".surql": 1.35,
    ".rs": 1.25,
    ".js": 1.10,
    ".css": 0.35,
    ".json": 0.30,
    ".md": 0.10,
    ".txt": 0.50
  },
  "search_path_boosts": [
    { "pattern": "src/", "weight": 1.55 },
    { "pattern": "src/components/", "weight": 1.85 },
    { "pattern": "src/components/preferences/daisyThemesRaw/", "weight": 0.30 },
    { "pattern": "src/locale/", "weight": 0.15 },
    { "pattern": "src/views/", "weight": 1.25 },
    { "pattern": "components/", "weight": 1.25 },
    { "pattern": "views/", "weight": 1.15 },
    { "pattern": "pages/", "weight": 1.15 },
    { "pattern": "routes/", "weight": 1.10 },
    { "pattern": "models/", "weight": 1.10 },
    { "pattern": "middleware/", "weight": 1.10 },
    { "pattern": "public/styles/", "weight": 0.05 },
    { "pattern": "docs/", "weight": 0.05 },
    { "pattern": "console-export", "weight": 0.60 },
    { "pattern": "Daisy_llms.txt", "weight": 0.60 }
  ],
  "search_language_boosts": {
    "vue": 1.65,
    "typescript": 1.30,
    "css": 0.50,
    "json": 0.55,
    "rust": 1.05
  },
  "search_exclude_patterns": [
    "console-export",
    "Daisy_llms.txt"
  ],
  "search_snippet_preview_chars": 200
}
